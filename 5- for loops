#include <iostream>

using namespace std;

int main()
{
    /*
     * For Loops
     * for(initialization; condition; increment/decrement;){
     * instruction to repeat;
     * }
     */

    for(int i = 0; i < 10; i++){
        cout << i << endl;
    }// will print from 0-9

    for(int i = 0; i <= 10; i++){
        cout << i << endl;
    }// will print from 0-10


    return 0;
}
