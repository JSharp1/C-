#include <iostream>

using namespace std;

int main()
{
    int i = 0;
    
    while(i<10){
        cout << i << endl;
        i++;
    }
    
    cout << endl;
    
    /*
     * Do While
     * the condition is checked at the end
     * of execution
     */
    
    do{
        cout << i << endl;
        i--;
    } while(i>0);
   
    return 0;
}
